kun= input("bugun nima kun?>>>")
harorat= float(input("havo harorati qanday?"))
if kun.lower()== 'shanba' and harorat>=30:
    print("bugun chomilamiz")
elif kun.lower()=='shanba'and harorat<30:
    print("bugun dam olamiz")
